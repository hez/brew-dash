<!DOCTYPE html>
<html lang="en">
  <head>
    <%= csrf_meta_tag() %>
    <%= live_title_tag(assigns[:page_title] || "Admin Brew Dash") %>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link phx-track-static rel="stylesheet" href={Routes.static_path(@conn, "/assets/admin.css")} />
    <script
      defer
      phx-track-static
      type="text/javascript"
      src={Routes.static_path(@conn, "/assets/admin.js")}
    >
    </script>
  </head>
  <body>
    <%= @inner_content %>
  </body>
</html>
